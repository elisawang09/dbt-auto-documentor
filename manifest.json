        "relation_name": "`codementor-warehouse-184604`.`dbt_staging`.`batch__job_candidate_by_source_and_review`",
        "raw_code": "SELECT\n    candidate.job_candidate_key\n    , candidate.user_id\n    , candidate.developer_id\n    , candidate.team_id\n    , candidate.job_request_id\n    , candidate.source AS candidate_source\n    , candidate.score AS candidate_score\n    , candidate.is_from_shortlist\n    , candidate.score_passes_threshold\n    , candidate.batch_id\n    , candidate.item_created_at\n    , candidate.model_version\n    , IF(review_stats.ivr_count > 0, TRUE, FALSE) AS has_ivr\n    , CASE\n        WHEN review_stats.liked_count > 0 THEN 'like'\n        WHEN review_stats.not_interested_count > 0 THEN 'not_interested'\n        ELSE NULL END AS client_feedback\n    , IF(review_stats.ivr_count + review_stats.not_interested_count + review_stats.liked_count  > 0, TRUE, FALSE) AS has_reviewed\n    , IF(client_rejected_count > 1, TRUE, FALSE) AS client_rejected_after_interview\n    , review_stats.candidate_rejection_reasons\n    , review_stats.candidate_rejection_other_reason\n    , review_stats.first_reviewed_candidate_at\n    , review_stats.first_viewed_candidate_at\n    , review_stats.first_viewed_candidate_at IS NOT NULL AS has_viewed_candidate\nFROM {{ ref(\"int_batch__job_candidate_by_source\") }} AS candidate\nLEFT JOIN {{ ref(\"int_batch__job_candidate_by_source_and_review\") }} AS review_stats\nUSING (job_candidate_key)\n-- exclude spam account's candidates\nWHERE job_candidate_key IS NOT NULL",
        "language": "sql",
